{
  "tasks": {
    "start": "deno run -A --watch=static/,routes/ dev.ts",
    "build": "deno run -A dev.ts build",
    "preview": "deno run -A main.ts",
    "update": "deno run -A -r https://fresh.deno.dev/update"
  },
  "compilerOptions": { "jsx": "react-jsx", "jsxImportSource": "preact" },
  "imports": {
    "$fresh/": "https://deno.land/x/fresh@1.7.3/",
    "$ga/": "https://deno.land/x/g_a@0.1.2/",
    "$std/": "https://deno.land/std@0.224.0/",
    "$tabler/icons/": "https://deno.land/x/tabler_icons_tsx@0.0.6/",
    "$zod": "https://deno.land/x/zod@v3.23.8/mod.ts",
    "@preact/signals": "https://esm.sh/*@preact/signals@1.3.0",
    "@preact/signals-core": "https://esm.sh/*@preact/signals-core@1.8.0",
    "gfm": "jsr:@deno/gfm@0.10",
    "preact": "https://esm.sh/preact@10.24.3",
    "preact/": "https://esm.sh/preact@10.24.3/",
    "preact-render-to-string": "https://esm.sh/*preact-render-to-string@6.5.11",
    "prismjs/": "https://esm.sh/prismjs@1.29.0/",
    "sharp": "npm:sharp@^0.33.5",
    "tailwindcss": "npm:tailwindcss@3.4.14",
    "tailwindcss/": "npm:/tailwindcss@3.4.14/",
    "tailwindcss/plugin": "npm:/tailwindcss@3.4.14/plugin.js"
  },
  "lint": { "rules": { "tags": ["fresh", "recommended"] } },
  "exclude": ["**/_fresh/*"],
  "nodeModulesDir": "auto"
}
